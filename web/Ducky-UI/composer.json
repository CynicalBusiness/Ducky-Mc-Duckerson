{
  "name": "ducky-mc-duckerson/ducky-ui",
  "description": "The web-ui for the Ducky Mc Duckerson Discord bot",
  "keywords": [
    "Discord",
    "bot",
    "Ducky"
  ],
  "homepage": "https://github.com/Mannjamin/Ducky-Mc-Duckerson",
  "license": "MIT",
  "authors": [
    {
      "name": "AbbyTheRat",
      "email": "some@email.com",
      "homepage": "https://github.com/AbbyTheRat"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "slim/slim": "^3.1",
    "slim/pdo": "^1.10",
    "slim/php-view": "^2.0",
    "oscarotero/psr7-middlewares": "^3.20",
    "monolog/monolog": "^1.17",
    "matthiasmullie/minify": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.8 < 6.0"
  },
  "autoload": {
    "psr-4": {
      "DuckyUI\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "start": "php -S 0.0.0.0:8080 -t public public/index.php",
    "test": "phpunit"
  }
}
